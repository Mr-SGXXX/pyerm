        </set_method_param>

        <data_select>
            <title>## Data Selection</title>
            <data_select>Available data:</data_select>
            <data_empty>**No data found in database**</data_empty>
            <add_new_data_checkbox>Add new data</add_new_data_checkbox>
            <add_new_data_input>New data name</add_new_data_input>
            <data_ok>Confirm</data_ok>
            <data_empty_error>Error: Data name cannot be empty.</data_empty_error>
        </data_select>

